a = {1, 2, 3, 4}
b = {3, 4, 5, 6}
c = {0, 0, 0, 0}

# a = {1, 2, 3, 4}
# b = {3, 4, 5, 6}
# c = {4, 5, 6, 7}
intersection = a & b 
print("intersection for file1 and file2",intersection) 

intersection = a & c 
print("intersection for file1 and file3",intersection)

intersection = c & b 
print("intersection for file3 and file2",intersection) 
